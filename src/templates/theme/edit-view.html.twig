{% extends "content.html.twig" %}
{% block content %}
    <div class="row">
        <div class="col-lg-8">
            <div class="view view-cascade gradient-card-header blue-gradient narrower py-2 mx-4 mb-3 d-flex justify-content-between align-items-center card-head">
                <div></div>
                <div class="white-text mx-3 tab-header">{% block table_name %}{% endblock %}</div>
                <div></div>
            </div>
            <div class="card card-cascade narrower card-cont">
                {% block table_nav %}{% endblock %}
                <div class="table-wrapper scrollbar-primary content-table">
                    <table class="table table-hover mb-0">
                        <thead>
                            <tr>
                                {% block table_header %}{% endblock %}
                            </tr>
                        </thead>
                        <tbody>
                            {% block table_body %}{% endblock %}
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
        <div class="col-lg-4">
            {% block right_col %}{% endblock %}
        </div>
    </div>
    {% block modals %}{% endblock %}
{% endblock %}